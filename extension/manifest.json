{
  "manifest_version": 3,
  "name": "Internet Proximity Chat",
  "version": "1.0",
  "description": "Chat with people on the same domain!",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
